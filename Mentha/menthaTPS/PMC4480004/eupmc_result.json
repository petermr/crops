iationDetailsList": {"authorAffiliation": {"affiliation": "Rothamsted Research, Harpenden, AL5 2JQ, UK."}}}, {"fullName": "Woodcock CM", "firstName": "Christine M", "lastName": "Woodcock", "initials": "CM", "authorAffiliationDetailsList": {"authorAffiliation": {"affiliation": "Rothamsted Research, Harpenden, AL5 2JQ, UK."}}}, {"fullName": "Birkett MA", "firstName": "Michael A", "lastName": "Birkett", "initials": "MA", "authorId": {"@type": "ORCID", "#text": "0000-0002-3302-2025"}, "authorAffiliationDetailsList": {"authorAffiliation": {"affiliation": "Rothamsted Research, Harpenden, AL5 2JQ, UK."}}}, {"fullName": "Napier JA", "firstName": "Johnathan A", "lastName": "Napier", "initials": "JA", "authorId": {"@type": "ORCID", "#text": "0000-0003-3580-3607"}, "authorAffiliationDetailsList": {"authorAffiliation": {"affiliation": "Rothamsted Research, Harpenden, AL5 2JQ, UK."}}}, {"fullName": "Jones HD", "firstName": "Huw D", "lastName": "Jones", "initials": "HD", "authorId": {"@type": "ORCID", "#text": "0000-0002-7030-0776"}, "authorAffiliationDetailsList": {"authorAffiliation": {"affiliation": "Rothamsted Research, Harpenden, AL5 2JQ, UK."}}}, {"fullName": "Pickett JA", "firstName": "John A", "lastName": "Pickett", "initials": "JA", "authorAffiliationDetailsList": {"authorAffiliation": {"affiliation": "Rothamsted Research, Harpenden, AL5 2JQ, UK."}}}]}, "authorIdList": {"authorId": [{"@type": "ORCID", "#text": "0000-0001-6068-4123"}, {"@type": "ORCID", "#text": "0000-0002-3302-2025"}, {"@type": "ORCID", "#text": "0000-0002-7030-0776"}, {"@type": "ORCID", "#text": "0000-0002-9912-0605"}, {"@type": "ORCID", "#text": "0000-0003-3580-3607"}]}, "dataLinksTagsList": {"dataLinkstag": ["supporting_data", "altmetrics"]}, "journalInfo": {"volume": "5", "journalIssueId": "2476015", "dateOfPublication": "2015 Jun", "monthOfPublication": "6", "yearOfPublication": "2015", "printPublicationDate": "2015-06-01", "journal": {"title": "Scientific reports", "ISOAbbreviation": "Sci Rep", "me